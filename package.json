{
  "name": "catalog-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:setup": "echo 'Please set up your Supabase project and update .env.local with your credentials'",
    "db:migrate": "echo 'Run database migrations using Supabase CLI or Studio'",
    "test:supabase": "node test-supabase.js",
    "test:cloudinary": "node -r tsx test-cloudinary.ts",
    "db:add-missing": "node add-missing-data.js",
    "db:init-settings": "node -r tsx scripts/init-general-settings-fixed.ts",
    "db:seed-admin": "node -r tsx src/scripts/seed-admin-users.ts",
    "db:set-user-role": "node -r tsx scripts/set-user-role.ts",
    "db:set-user-role-email": "SET_BY_EMAIL=true node -r tsx scripts/set-user-role.ts",
    "cache:clear": "node scripts/clear-cache.js",
    "db:create-homepage-blocks": "bash create-homepage-blocks-tables.sql",
    "db:migrate-homepage": "bash run-homepage-blocks-migration.sh",
    "db:update-homepage-rls": "bash update-homepage-blocks-rls-policies.sh",
    "db:update-category-product-order-rls": "bash update-category-product-order-rls-policies.sh"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "papaparse": "^5.5.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-dropzone": "^14.3.8",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.28",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@types/xlsx": "^0.0.35",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
